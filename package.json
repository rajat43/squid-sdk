{
  "name": "@0xsquid/sdk",
  "version": "1.0.0-beta.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xsquid/api-sdk.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts,.json .",
    "lint:fix": "eslint --ext .js,.ts,.json . --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "husky install",
    "test": "jest ./*/*.spec.ts",
    "build": "tsc",
    "integration": "npx ts-node src/scripts/test-sdk.ts",
    "integration:sendtrade": "npx ts-node src/scripts/test-sdk.ts -s sendtrade",
    "integration:tradesend": "npx ts-node src/scripts/test-sdk.ts -s tradesend",
    "integration:tradesendtrade": "npx ts-node src/scripts/test-sdk.ts -s tradesendtrade"
  },
  "dependencies": {
    "@axelar-network/axelarjs-sdk": "^0.6.18",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "assert": "^2.0.0",
    "babel-jest": "^28.1.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "description": "## Integration Tests",
  "bugs": {
    "url": "https://github.com/0xsquid/api-sdk/issues"
  },
  "homepage": "https://github.com/0xsquid/api-sdk#readme",
  "author": ""
}
